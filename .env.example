# LiveKit connection settings
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# Ollama settings (default local)
OLLAMA_BASE_URL=http://localhost:11434/v1

# TTS Engine: "kokoro" (default), "vibevoice", or "cartesia"
VAGENT_TTS_ENGINE=kokoro

# Kokoro TTS settings (used when VAGENT_TTS_ENGINE=kokoro)
VAGENT_KOKORO_URL=http://localhost:8880/v1
VAGENT_KOKORO_VOICE=af_heart

# VibeVoice TTS settings (used when VAGENT_TTS_ENGINE=vibevoice)
# Run VibeVoice server: python demo/vibevoice_realtime_demo.py --model_path microsoft/VibeVoice-Realtime-0.5B --port 3000
VAGENT_VIBEVOICE_URL=ws://localhost:3000
VAGENT_VIBEVOICE_VOICE=en-WHTest_man
VAGENT_VIBEVOICE_CFG=1.5
VAGENT_VIBEVOICE_STEPS=5

# Cartesia settings (used when VAGENT_TTS_ENGINE=cartesia and/or VAGENT_STT_ENGINE=cartesia)
VAGENT_CARTESIA_API_KEY=
VAGENT_CARTESIA_VERSION=2025-04-16

# Cartesia TTS settings (used when VAGENT_TTS_ENGINE=cartesia)
# NOTE: voice_id is required (UUID from Cartesia voice library)
VAGENT_CARTESIA_VOICE_ID=
VAGENT_CARTESIA_TTS_MODEL=sonic-3
VAGENT_CARTESIA_TTS_LANGUAGE=en
VAGENT_CARTESIA_TTS_SAMPLE_RATE=24000
VAGENT_CARTESIA_TTS_MAX_BUFFER_DELAY_MS=3000

# STT Engine: "whisper" (default) or "cartesia"
VAGENT_STT_ENGINE=whisper

# STT settings
VAGENT_STT_BEAM_SIZE=1
VAGENT_STT_VAD_FILTER=1
VAGENT_STT_WITHOUT_TIMESTAMPS=1

# Cartesia STT settings (used when VAGENT_STT_ENGINE=cartesia)
VAGENT_CARTESIA_STT_MODEL=ink-whisper
VAGENT_CARTESIA_STT_LANGUAGE=en
VAGENT_CARTESIA_STT_SAMPLE_RATE=16000
VAGENT_CARTESIA_STT_MIN_VOLUME=0.0
VAGENT_CARTESIA_STT_MAX_SILENCE_SECS=0.8

# Agent settings
VAGENT_MIN_ENDPOINTING_DELAY=0.1
VAGENT_LATENCY=1
